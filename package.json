{
  "name": "cosmic-insights",
  "version": "3.0.0",
  "description": "ðŸ”® Revolutionary AI-powered spiritual guidance platform with advanced astrology, numerology, tarot, dream interpretation, and cosmic intelligence - 2025 Enterprise Edition",
  "main": "server.js",
  "type": "module",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "build": "npm run build:client && npm run build:server",
    "build:client": "vite build",
    "build:server": "tsc && node scripts/optimize-server.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:e2e": "playwright test",
    "lint": "eslint . --ext .js,.ts,.html",
    "lint:fix": "eslint . --ext .js,.ts,.html --fix",
    "format": "prettier --write \"**/*.{js,ts,html,css,json,md}\"",
    "type-check": "tsc --noEmit",
    "analyze": "vite-bundle-analyzer",
    "audit": "npm audit --audit-level moderate",
    "security": "npm run audit && node scripts/security-scan.js",
    "validate:readings": "node scripts/validate-spiritual-data.js",
    "benchmark:ai": "node scripts/benchmark-ai-performance.js",
    "monitor": "node scripts/monitor-cosmic-analytics.js",
    "deploy:staging": "vercel",
    "deploy:production": "vercel --prod",
    "docker:build": "docker build -t cosmic-insights:latest .",
    "docker:run": "docker run -p 3000:3000 cosmic-insights:latest",
    "health-check": "node scripts/health-check.js",
    "clean": "rimraf dist coverage .vercel"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Gzeu/cosmicinsights.git"
  },
